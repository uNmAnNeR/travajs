{
  "author": "Alexey Kryazhev",
  "name": "trava",
  "version": "1.0.0",
  "license": "MIT",
  "description": "vanilla javascript validation library",
  "main": "dist/trava.js",
  "bugs": "https://github.com/uNmAnNeR/travajs/issues",
  "homepage": "https://unmanner.github.io/travajs/",
  "repository": "https://github.com/uNmAnNeR/travajs",
  "types": "index.d.ts",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "documentation": "^9.1.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "flow-bin": "^0.93.0",
    "mocha": "^6.0.1",
    "nyc": "^13.3.0",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "sinon": "^7.2.4"
  },
  "engines": {
    "npm": ">=4.0.0",
    "node": ">=6.0.0"
  },
  "scripts": {
    "doc": "echo 'TODO'",
    "flow": "flow",
    "pretest": "flow check",
    "watch": "rollup -c -w",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "npm run build:dev:es && npm run build:dev:umd",
    "build:prod": "npm run build:prod:es && npm run build:prod:umd",
    "build:dev:es": "cross-env BABEL_ENV=es rollup -c",
    "build:dev:umd": "rollup -c",
    "build:prod:es": "cross-env NODE_ENV=production BABEL_ENV=es rollup -c",
    "build:prod:umd": "cross-env NODE_ENV=production rollup -c",
    "test": "cross-env NODE_ENV=test nyc mocha --exit --recursive",
    "make": "npm run test && npm run build",
    "release": "npm run doc && npm run make && npm publish",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "javascript",
    "validate"
  ]
}
